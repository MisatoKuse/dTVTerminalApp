<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    style="@style/default_layout_style"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/firstLanchLanchYesActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="#FFFFFF"
        android:text="初回(チュートリアル)" />

    <Button
        android:id="@+id/firstLanchLanchNoActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="#FFFFFF"
        android:text="チュートリアル確認済" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="40dp"
        android:text="Splash,チュートリアル,利用規約等は現時点未実装のためこれはダミーの画面です."
        android:textColor="#FFFFFF"/>

</LinearLayout>