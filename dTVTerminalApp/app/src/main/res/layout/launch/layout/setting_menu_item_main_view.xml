<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/default_layout_style"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <WebView
        android:id="@+id/setting_menu_main_webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <RelativeLayout
        android:id="@+id/webview_function_menu"
        android:layout_width="match_parent"
        android:layout_height="@dimen/launch_text_layout_high_size"
        android:layout_alignParentBottom="true"
        android:layout_gravity="bottom"
        android:visibility="gone">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/backgrand_webview_function_menu" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="44dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:gravity="end"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/function_menu_back"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="78dp"
                android:layout_marginTop="14dp" />

            <ImageView
                android:id="@+id/function_menu_next"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="78dp"
                android:layout_marginTop="14dp" />

            <ImageView
                android:id="@+id/function_menu_reload"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="38dp"
                android:layout_marginTop="14dp"
                android:background="@drawable/icon_webview_function_menu_reload_active" />

        </LinearLayout>
    </RelativeLayout>
</RelativeLayout>