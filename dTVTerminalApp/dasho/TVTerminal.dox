        <android name="com.nttdocomo.android.tvterminalapp.activity.home.RecordedListActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.home.RentalListActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.home.WatchingVideoListActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.launch.DAccountReSettingActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.launch.DAccountSettingHelpActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.launch.LaunchActivity"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.launch.PairingHelpActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.launch.STBConnectActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.launch.STBSelectActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.launch.STBSelectErrorActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.launch.TutorialActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.ranking.DailyTvRankingActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.ranking.RankingTopActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.ranking.VideoRankingActivity" rename-class="true"/>
