            </classes>
            <classes name="com.nttdocomo.android.tvterminalapp.jni.DlnaInterface">
                <method name="notifyFromNative" signature="int,java.lang.String"/>
                <method name="notifyObjFromNative" signature="int,java.util.ArrayList"/>
            </classes>
            <classes name="com.nttdocomo.android.tvterminalapp.jni.download.DlnaInterfaceDl">
                <method name="notifyFromNative" signature="int,java.lang.String"/>
                <method name="getUniqueId" signature=""/>
            </classes>
        </excludelist>
        <mapping>
            <mapreport path="${dasho.results.dir}/${projectname}-mapreport.txt"/>
            <mapoutput overwrite="true" path="${dasho.results.dir}/${projectname}.map"/>
        </mapping>
    </renaming>
    <optimization option="on"/>
    <controlflow option="on" tryCatch="on"/>
    <stringencrypt option="on">
        <seOutput path="${dasho.results.dir}/${projectname}-se.map"/>
    </stringencrypt>
    <injection>
        <instrumentation option="off">
            <runtime target="android4"/>
        </instrumentation>
    </injection>
</dasho>
