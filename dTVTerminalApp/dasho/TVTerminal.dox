                <method name="**" signature="**"/>
            </classes>
            <classes name="com.digion.dixim.android.player.R">
                <field name="**"/>
                <method name="**" signature="**"/>
            </classes>
            <classes name="com.digion.dixim.android.player.R$*">
                <field name="**"/>
                <method name="**" signature="**"/>
            </classes>
        </excludelist>
    </removal>
    <methodCallRemoval/>
    <renaming option="on">
        <class-options randomize="true" prefix="${manifest.package}."/>
        <member-options randomize="true"/>
        <excludelist>
            <classes name="com.digion.dixim.android.player.secure.openmaxal.R">
                <field name="**"/>
                <method name="**" signature="**"/>
            </classes>
            <classes name="com.digion.dixim.android.player.secure.openmaxal.R$*">
                <field name="**"/>
                <method name="**" signature="**"/>
            </classes>
            <classes name="com.digion.dixim.android.util.R">
                <field name="**"/>
                <method name="**" signature="**"/>
            </classes>
            <classes name="com.digion.dixim.android.util.R$*">
                <field name="**"/>
                <method name="**" signature="**"/>
            </classes>
            <classes name="com.digion.dixim.android.player.secure.nativecodec.R">
                <field name="**"/>
                <method name="**" signature="**"/>
            </classes>
            <classes name="com.digion.dixim.android.player.secure.nativecodec.R$*">
                <field name="**"/>
                <method name="**" signature="**"/>
            </classes>
            <classes name="com.digion.dixim.android.activation.R">
                <field name="**"/>
                <method name="**" signature="**"/>
            </classes>
            <classes name="com.digion.dixim.android.activation.R$*">
