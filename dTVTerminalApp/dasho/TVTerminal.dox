<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<dasho version="8.2.0">
    <propertylist>
        <property name="projectname" value="dasho"/>
        <property name="ANDROID_BUILD_TOOLS_HOME" value="${sdk.dir}\build-tools\26.0.2"/>
        <property name="apkOutput" value="TVTerminal-obfuscated.apk"/>
        <property name="apkInput" value="..\app\build\outputs\apk\TVTerminal.apk"/>
        <property name="sdk.dir" value="${ANDROID_SDK_DIR}"/>
        <property name="manifest.package" value="com.nttdocomo.android.tvterminalapp"/>
        <property name="dasho.results.dir" value="dasho-results_TVTerminal"/>
    </propertylist>
    <global>
        <option>IGNORENOTFOUNDCLASSES</option>
        <option>FORNAMEDETECTION</option>
        <option>FORCE</option>
    </global>
    <inputpath>
        <pathelement location="${apkInput}"/>
    </inputpath>
    <globalProcessingExclude>
        <classes name="android.**"/>
        <classes name="com.nttdocomo.android.ocsplib.*"/>
        <classes name="com.nttdocomo.android.tvterminalapp.jni.*"/>
        <classes name="com.nttdocomo.android.tvterminalapp.jni.activation.*"/>
        <classes name="com.nttdocomo.android.tvterminalapp.jni.download.*"/>
        <classes name="com.nttdocomo.android.tvterminalapp.jni.remote.*"/>
    </globalProcessingExclude>
    <classpath appendrtjar="false">
        <pathelement location="${sdk.dir}\platforms\android-26\android.jar"/>
    </classpath>
    <entrypoints>
        <android name="com.digion.dixim.android.activation.ActivationActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.detail.ChannelDetailPlayerActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.detail.ContentDetailActivity" rename-class="true"/>
