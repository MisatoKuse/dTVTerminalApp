        <android name="com.nttdocomo.android.tvterminalapp.activity.search.SearchTopActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.setting.ApplicationInfoActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.setting.NewsActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.setting.NewsDetailActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.setting.SettingActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.setting.SettingImageQualityActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.setting.SettingMenuAppActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.setting.SettingMenuFaqActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.setting.SettingMenuLicenseActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.setting.SettingMenuPrivacyPolicyActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.setting.SettingMenuTermsOfServiceActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.tvprogram.ChannelListActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.tvprogram.MyChannelEditActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.tvprogram.TvProgramListActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.tvprogram.WeekTvProgramListActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.video.VideoContentListActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.activity.video.VideoTopActivity" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.service.download.DownloadService" rename-class="true"/>
        <android name="com.nttdocomo.android.tvterminalapp.webapiclient.daccount.DaccountReceiver"/>
        <classes name="com.digion.dixim.android.secureplayer.SecureVideoView">
            <method name="&lt;init&gt;" signature="**" modifiers="public"/>
        </classes>
        <classes name="com.nttdocomo.android.ocsplib.bouncycastle.LICENSE">
            <method name="main" signature="java.lang.String[]" modifiers="public static"/>
        </classes>
        <classes name="com.nttdocomo.android.tvterminalapp.view.NoScrollViewPager">
            <method name="&lt;init&gt;" signature="**" modifiers="public"/>
        </classes>
        <classes name="com.nttdocomo.android.tvterminalapp.view.ProgramRecyclerView">
            <method name="&lt;init&gt;" signature="**" modifiers="public"/>
        </classes>
        <classes name="com.nttdocomo.android.tvterminalapp.view.ProgramScrollView">
            <method name="&lt;init&gt;" signature="**" modifiers="public"/>
        </classes>
        <classes name="com.nttdocomo.android.tvterminalapp.view.RecommendListView">
            <method name="&lt;init&gt;" signature="**" modifiers="public"/>
        </classes>
        <classes name="com.nttdocomo.android.tvterminalapp.view.RemoteControllerView">
            <method name="&lt;init&gt;" signature="**" modifiers="public"/>
        </classes>
        <classes name="**" rename="true">
            <method name="contentsDetailBackKey" signature="android.view.View" modifiers="public"/>
            <method name="contractButtonClick" signature="android.view.View" modifiers="public"/>
        </classes>
    </entrypoints>
    <report path="${dasho.results.dir}/${projectname}-report.txt"/>
    <output merge="false" compress="false" level="0">
        <apk path="${apkOutput}"/>
    </output>
    <removal classes="unused" members="unused-non-public">
        <debug/>
        <attributes/>
        <excludelist>
            <classes name="com.digion.dixim.android.player.secure.openmaxal.R">
                <field name="**"/>
                <method name="**" signature="**"/>
            </classes>
            <classes name="com.digion.dixim.android.player.secure.openmaxal.R$*">
                <field name="**"/>
                <method name="**" signature="**"/>
            </classes>
            <classes name="com.digion.dixim.android.util.R">
                <field name="**"/>
